<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.xdu.debateteam.mapper.UserMapper">

    <select id="selectByIdInTeamer" resultType="edu.xdu.debateteam.pojo.User">
        select *
        from teamer
        where id = #{id};
